<p id="notice"><%= notice %></p>

<h1>User Notes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_notes.each do |user_note| %>
      <tr>
        <td><%= link_to 'Show', user_note %></td>
        <td><%= link_to 'Edit', edit_user_note_path(user_note) %></td>
        <td><%= link_to 'Destroy', user_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Note', new_user_note_path %>
