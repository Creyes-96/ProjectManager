<p id="notice"><%= notice %></p>

<h1>Active users</h1>

<table class="table">
  <thead>
    <tr>
      <th class="">First name</th>
      <th class="">Last name</th>
      <th class="">Username</th>
      <th class="">Email</th>
      <th class="">Privilege</th>
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td class=""><%= user.first_name %></td>
        <td class=""><%= user.last_name %></td>
        <td class=""><%= user.username %></td>
        <td class=""><%= user.email %></td>
        <td class=""><%= user.privilege %></td>
        <% if can? :manage, user%>
        <td><%= link_to 'Show'%></td>
        <td><%= link_to 'Edit', edit_registration_url(user) %></td>
        <td><%= link_to 'Destroy'%></td>
        <% end %>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

